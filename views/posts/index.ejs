                    <h5 class="card-title">
                        <%=post.title%></h5>
                    <p class="card-text"><%=post.price %></p>
                    <p class="card-text"><%=post.description%></p>
                    <p class="card-text"><%=post.location%></p>
                  </div>
                </div>

    <% });%>
            </div>
        
